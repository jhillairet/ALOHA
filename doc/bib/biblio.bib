@Article{Klima1980,
  Title                    = {{Nonlinear generation of spatial harmonics at the edge of lower hybrid heated plasma}},
  Author                   = {Klima, R. and Pankratov, I. M. and Pavlo, P. and Petrzilka, V. A.},
  Journal                  = {Plasma Physics},
  Year                     = {1980},
  Number                   = {12},
  Pages                    = {1109},
  Volume                   = {22},

  Abstract                 = {A large amplitude wave strongly modulates, due to ponderomotive forces, the plasma density in the low-temperature boundary layer (Morales, 1977; Chan and Chiu, 1979). These density modifications at the plasma edge are important for the lower hybrid waves propagation (Bers 1978), cf. the case studied experimentally in the Alcator Tokamak (Surko et al., 1979). The authors point out that the action of the ponderomotive forces in the boundary layer leads to generation of (at least one) spatial harmonic which propagates to the plasma interior. This effect stems from the z-dependence of nonlinear density modulations, which has not been included in the theoretical papers cited above. As the Landau damping is very sensitive to the value of the z-component of the wave vector, the spatial harmonics can be absorbed much more strongly than the original wave.},
  Url                      = {http://stacks.iop.org/0032-1028/22/i=12/a=007}
}

