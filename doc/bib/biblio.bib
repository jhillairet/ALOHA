@Proceedings{Troyon1975,
  Title                    = {{Lower-hybrid heating in large Tokamak}},
  Year                     = {1975},
  Editor                   = {{Troyon}, F. and {Perkins}, F.~W.},

  Abstract                 = {{RF heating in the vicinity of the lower-hybrid resonance frequency has been shown to be an efficient method to heat plasmas. The best results are obtained when the frequency is a few times (2-4) the maximum lower hybrid frequency. The temperature profile is then peaked in the center while it becomes hollow at lower frequencies. Nonlinear processes are responsible for the heating, probably through parametric decays and nonlinear Landau damping. The extrapolation of this technique to the large Tokamaks which are being built and to the larger ones contemplated as reactors encounters new problems of accessibility which are examined. }},
